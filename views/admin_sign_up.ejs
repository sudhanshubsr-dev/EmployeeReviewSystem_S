<link rel="stylesheet" href="/css/sign_up.css">

<main>
    <section>
    <div id="sign-up-form">
        <h3>Sign-Up</h3>
        <form action="/admin/create" method="POST" id="sign-up">
            <div class="name-form">
                <label for="name">Name</label>
                <input type="text" name="name" placeholder="Username" required>
            </div>
            <div class="email-form">
                <label for="email">Email</label>
                <input type="email" name="email" placeholder="Email" required>
            </div>
            <div class="password-form">
                <label for="password">Password</label>
                <input type="password" name="password" placeholder="Password" required>
            </div>
            <div class="confirm-password-form">
                <label for="confirm-password">Confirm-Password</label>
                <input type="text" name="confirm-password" placeholder="Confirm-Password" required>
            </div>
            <div class="passcode-form">
                <label for="passcode">Passcode</label>
                <input type="text" name="passcode" placeholder="Passcode" required>
            </div>
            <button class="btn">Sign Up</button>
        </form>
    </div>
    </section>
</main>
